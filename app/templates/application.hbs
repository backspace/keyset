<h2 id="title">Transactions</h2>

<table>
  <tr>
    <th>ID</th>
    <th>Date</th>
    <th>Amount</th>
    <th>Description</th>
    <th>Balance</th>
  </tr>
  {{#each tx in model}}
    <tr>
      <td>{{tx.id}}</td>
      <td>{{tx.formattedDate}}</td>
      <td>{{tx.amount}}</td>
      <td>{{tx.description}}</td>
      <td>{{tx.balance}}</td>
    </tr>
  {{/each}}
</table>

<button {{action 'prev'}}>Prev Page</button>
<button {{action 'next'}}>Next Page</button>

{{outlet}}
